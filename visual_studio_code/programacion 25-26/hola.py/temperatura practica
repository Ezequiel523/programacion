temperatura=float(input("dime la temperatura"))
if temperatura>26:
   print("enciendo el aire")
   temperatura=float(input("dime la temperatura"))
   if temperatura<23:
    print("no enciende el aire")
elif temperatura<10:
      print("encender calefaccion")
else:
    print("no encender el aire")
print("registro"+str(temperatura))
  

